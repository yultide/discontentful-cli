{
	"name": "contentful-tools-cli",
	"version": "0.0.1",
	"private": true,
	"description": "",
	"repository": {
		"type": "git",
		"url": ""
	},
	"type": "module",
	"bin": {
		"ct": "./dist/index.js",
		"contentful-tools-cli": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm --clean --minify --metafile",
		"changeset": "changeset",
		"dev": "tsup src/index.ts --format esm --watch --clean --onSuccess \"node dist/index.js\"",
		"prepare": "husky install",
		"release": "changeset publish",
		"lint": "eslint",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"cli": "tsx src/index.ts"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,mdx,yml,json}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@inquirer/prompts": "^7.3.2",
		"chalk": "^5.4.1",
		"commander": "^13.1.0",
		"contentful-management": "^11.47.2",
		"exceljs": "^4.4.0",
		"figlet": "^1.5.2",
		"ora": "^8.2.0"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@commitlint/cli": "^17.4.1",
		"@commitlint/config-conventional": "^17.4.0",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/figlet": "^1.5.5",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"@vitest/coverage-v8": "3.0.6",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^5.2.3",
		"husky": "^9.1.7",
		"lint-staged": "^13.1.0",
		"prettier": "^3.5.1",
		"tsup": "^6.5.0",
		"tsx": "^4.19.3",
		"type-fest": "^3.5.1",
		"typescript": "^4.9.4",
		"vitest": "^3.0.6"
	},
	"engines": {
		"node": ">=14.16"
	}
}
