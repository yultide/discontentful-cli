{
	"name": "@discontentful/cli",
	"version": "0.1.0",
	"license": "MIT",
	"description": "A collection of contentful tools and utlitiy functions to easily import and export content from an excel spreadsheet.",
	"repository": {
		"type": "git",
		"url": ""
	},
	"type": "module",
	"bin": {
		"dc": "dist/index.js",
		"discontentful": "dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc && tsc-alias --resolve-full-paths && npm pack",
		"changeset": "changeset",
		"prepare": "husky",
		"release": "changeset publish",
		"lint": "eslint && tsc --noEmit",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"cli": "tsx src/index.ts"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,mdx,yml,json}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@babel/parser": "^7.26.10",
		"@contentful/rich-text-html-renderer": "^17.0.0",
		"@contentful/rich-text-types": "^17.0.0",
		"@inquirer/prompts": "^7.4.0",
		"chalk": "^5.4.1",
		"commander": "^13.1.0",
		"contentful-management": "^11.48.0",
		"diff": "^7.0.0",
		"exceljs": "^4.4.0",
		"figlet": "^1.5.2",
		"lodash": "^4.17.21",
		"mime-types": "^2.1.35",
		"mkdirp": "^3.0.1",
		"nanoid": "^5.1.3",
		"node-fetch": "^3.3.2",
		"ora": "^8.2.0",
		"prettier": "^3.5.3",
		"recast": "^0.23.11",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"sharp": "^0.33.5",
		"table": "^6.9.0",
		"to-ast": "^1.0.0",
		"unified": "^11.0.5"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@eslint/js": "^9.22.0",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/diff": "^7.0.1",
		"@types/figlet": "^1.5.5",
		"@types/lodash": "^4.17.16",
		"@types/mime-types": "^2.1.4",
		"@types/node": "^22.13.10",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"@vitest/coverage-v8": "3.0.8",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"tsc-alias": "^1.8.11",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"type-fest": "^4.37.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vitest": "^3.0.8",
		"vitest-console": "^0.2.0"
	},
	"engines": {
		"node": ">=14.16"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"sharp"
		]
	},
	"main": "index.js",
	"directories": {
		"test": "tests"
	},
	"author": ""
}
